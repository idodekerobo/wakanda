{
  "expo": {
    "name": "Afe",
    "slug": "afe",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "0.4.7",
    "orientation": "portrait",
    "icon": "./assets/afe-v4-icon.png",
    "splash": {
      "image": "./assets/afe-v4.png",
      "resizeMode": "contain",
      "backgroundColor": "#105927"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "io.afeapp.afe",
      "buildNumber": "0.5.1",
      "supportsTablet": false,
      "infoPlist": {
         "NSLocationWhenInUseUsageDescription": "This app uses the user's location to show the user businesses in their area and how far it is from their location."
       }
    }
  }
}